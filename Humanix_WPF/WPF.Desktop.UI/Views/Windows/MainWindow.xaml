<Window x:Class="WPF.Desktop.UI.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.Desktop.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Center" FontSize="50">MAIN</TextBlock>
        <DockPanel VerticalAlignment="Top">
            <Frame NavigationUIVisibility="Hidden" x:Name="MainFrame"/>

            <!--Content="{Binding Path=DisplayPage, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"-->
        </DockPanel>
    </Grid>
</Window>
